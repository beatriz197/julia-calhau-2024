#include <stdio.h>

int main() {
  
    int valores[5];
    int soma = 0;

    for (int i = 0; i < 5; i++) {
        printf("Digite o %dº valor inteiro: ", i + 1);
        scanf("%d", &valores[i]);
    }

    for (int i = 0; i < 5; i++) {
        soma += valores[i];
    }

    printf("A soma dos valores é: %d\n", soma);

    return 0;
}
