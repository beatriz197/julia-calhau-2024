#include <stdio.h>

int main() {
    float valores[10];

    printf("Digite 10 valores reais:\n");
    for (int i = 0; i < 10; i++) {
        scanf("%f", &valores[i]);
    }

    printf("Valores na ordem inversa de entrada:\n");
    for (int i = 9; i >= 0; i--) {
        printf("%.2f\n", valores[i]);
    }

    return 0;
}
